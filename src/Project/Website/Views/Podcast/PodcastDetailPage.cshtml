@model Mvp.Feature.Media.Podcast.Models.PodcastModel

@if (Model == null)
{
    return;
}

<div class="container">
    <div class="jumbotron text-center">
        <h1>@Html.Raw(Model.PodcastName)</h1>
    </div>


    <div class="row">
        <div class="col-sm-12">
            <p>@Html.Raw(Model.PodcastTranscript)</p>
        </div>
    </div>
</div>